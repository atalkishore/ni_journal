<% 
showAd = process.env.SHOW_AD || "false";
ENABLE_CLARITY = process.env.ENABLE_CLARITY || "false";
ENABLE_NOTIFICATION = process.env.ENABLE_NOTIFICATION || "false";
imgDataUri = "data:image/png;base64,"
delayedDataInfo = "These feeds are not real-time. Please trade cautiously"
var now = new Date();
timeInMinute = (now.getUTCHours() * 60 + now.getUTCMinutes()) + 330
today=now;
isTradingHourNse = timeInMinute > 540 && timeInMinute < 930
isTradingHourNseExtended = timeInMinute > 480 && timeInMinute < 960
preMarketHour = timeInMinute > 8 * 60 && timeInMinute < (15 * 60 + 15)
postMarketHour = timeInMinute > 16 * 60
isWeekEnd = now.getDay() == 0 || now.getDay() == 6
isIndexSymbol=(symbol) =>{
    symbol = symbol.toUpperCase();
    return symbol === "NIFTY" || symbol === "BANKNIFTY" || symbol === "FINNIFTY" || symbol==="MIDCPNIFTY" || symbol === "SENSEX" || symbol==="BANKEX"
} 
isCurrOrCommoditySymbol=(symbol) =>{
    symbol = symbol.toUpperCase();
    return isCurrencySymbol(symbol) || isCommoditySymbol(symbol);
}
isCurrencySymbol=(symbol) =>{
    symbol = symbol.toUpperCase();
    return symbol === "USDINR";
}
isCommoditySymbol=(symbol) =>{
    symbol = symbol.toUpperCase();
    return symbol === "NATURALGAS" || symbol === "CRUDEOIL" || symbol === "GOLD" || symbol === "COPPER" 
    || symbol === "CRUDEOIL" || symbol === "CRUDEOILM" || symbol === "GOLDM" || symbol === "NATGASMINI" 
    || symbol === "SILVER"|| symbol === "SILVERM"|| symbol === "SILVER"|| symbol === "ZINC";
}
capitalizeWords = (str) => str.replace(/\w\S*/g,
        (txt) => txt.charAt(0).toUpperCase() + txt.substr(1).toLowerCase()
);
changeTextClass = (val) => val > 0 ? "text-success" : "text-danger";
bgClass = (val) => val > 0 ? "success" : "danger";
arrowName = (val) => (val > 0 ? "uil uil-arrow-growth mx-1" : "uil uil-chart-down mx-1");
themeColor = "bg-info"
oc_openInterest = 0;
oc_changeInOpenInterest=1;
oc_impliedVolatility=2;
oc_lastTradedPrice=3;
oc_netChange=4;
oc_volume=5;
oc_volumeInLot=6;
oc_openInterestInLot=7;
oc_changeInOpenInterestInLot=8;
oc_delta=9;
oc_gamma=10;
oc_theta=11;
oc_vega=12;
PYTHON_SERVICE_HOST=process.env.PYTHON_SERVICE_URL
%>